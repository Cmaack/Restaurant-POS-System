<h2>Please create a <strong>User Name</strong> and <strong>Password</strong> for the new employee.</h2>

  <div class="container">

<%= form_for @employee do |f| %>
<%= f.label :username, 'User Name: ' %>
  <%= f.text_field :username %>
  <%= f.label :password, 'Password: ' %>
  <%= f.password_field :password %>
  <%= f.submit %>
<% end %>
</div>
